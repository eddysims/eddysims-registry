    "fumadocs-core": "15.6.5",
    "fumadocs-mdx": "11.7.0",
    "fumadocs-ui": "15.6.5",
    "lucide-react": "^0.525.0",
    "next": "15.4.2",
    "next-themes": "^0.4.6",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-syntax-highlighter": "^15.6.1",
